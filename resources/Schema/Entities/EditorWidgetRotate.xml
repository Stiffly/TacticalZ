<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<Entity name="WidgetRotate" xmlns:c="components" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../Types/Entity.xsd">

  <Components>
    <c:Transform/>
  </Components>

  <Children>
    <Entity name="X">
      <Components>
        <c:EditorWidget>
          <MouseAxis X="0" Y="1" Z="0"/>
          <Type>
            <Rotate/>
          </Type>
          <Axis X="1" Y="0" Z="0"/>
        </c:EditorWidget>
        <c:Model>
          <Resource>Models/RotationWidgetX.obj</Resource>
        </c:Model>
        <c:Transform/>
      </Components>
      <Children/>
    </Entity>
    <Entity name="Y">
      <Components>
        <c:EditorWidget>
          <MouseAxis X="1" Y="0" Z="0"/>
          <Type>
            <Rotate/>
          </Type>
          <Axis X="0" Y="1" Z="0"/>
        </c:EditorWidget>
        <c:Model>
          <Resource>Models/RotationWidgetY.obj</Resource>
        </c:Model>
        <c:Transform/>
      </Components>
      <Children/>
    </Entity>
    <Entity name="Z">
      <Components>
        <c:EditorWidget>
          <MouseAxis X="1" Y="1" Z="0"/>
          <Type>
            <Rotate/>
          </Type>
          <Axis X="0" Y="0" Z="1"/>
        </c:EditorWidget>
        <c:Model>
          <Resource>Models/RotationWidgetZ.obj</Resource>
        </c:Model>
        <c:Transform/>
      </Components>
      <Children/>
    </Entity>
    <Entity name="Light">
      <Components>
        <c:PointLight>
          <Intensity>-1</Intensity>
          <Falloff>0</Falloff>
        </c:PointLight>
        <c:Transform/>
      </Components>
      <Children/>
    </Entity>
  </Children>

</Entity>
